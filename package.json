{
  "name": "amphibian-ten",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-prod": "ng build --prod",
    "test": "ng test --reporters junit --code-coverage",
    "lint": "ng lint",
    "lint-junit": "./node_modules/.bin/tslint -c ./tslint.json \"src/**/*.ts\" --format junit --out reports/lint-results.xml",
    "compodoc": "compodoc -p tsconfig.json --output reports/documentation/",
    "e2e": "ng e2e",
    "electron-build": "ng build --base-href ./ && cd ./src/electron && copyfiles ./* ./../../dist/amphibian-ten",
    "electron-build-prod": "ng build --prod --base-href ./ && cd ./src/electron && copyfiles ./* ./../../dist/amphibian-ten",
    "electron-start": "npm run electron-build && electron ./dist/amphibian-ten/.",
    "electron-linux": "npm run electron-build && electron-packager ./dist/amphibian-ten %npm_package_name% --icon=src/favicon.ico --platform=linux --out=out --prune=true --overwrite",
    "electron-linux-prod": "npm run electron-build-prod && electron-packager ./dist/amphibian-ten %npm_package_name% --icon=src/favicon.ico --platform=linux --out=out --prune=true --overwrite",
    "electron-macos": "npm run electron-build && electron-packager ./dist/amphibian-ten %npm_package_name% --icon=src/favicon.ico --platform=darwin --out=out --overwrite",
    "electron-macos-prod": "npm run electron-build-prod && electron-packager ./dist/amphibian-ten %npm_package_name% --icon=src/favicon.ico --platform=darwin --out=out --overwrite",
    "electron-windows": "npm run electron-build && electron-packager ./dist/amphibian-ten %npm_package_name% --icon=src/favicon.ico --platform=win32 --out=out --overwrite",
    "electron-windows-prod": "npm run electron-build-prod && electron-packager ./dist/amphibian-ten %npm_package_name% --icon=src/favicon.ico --platform=win32 --out=out --overwrite",
    "cordova-clean": "cd ./cordova && cordova clean",
    "cordova-android-prepare": "cd ./cordova && cordova platform remove android && cordova platform add android",
    "cordova-android": "cd ./cordova && cordova build android --debug",
    "cordova-android-prod": "cd ./cordova && cordova build android --release",
    "cordova-android-start": "npm run cordova-android && cd ./cordova && cordova emulate android",
    "release-notes": "mkdirp ./release && markdown ./src/releasenotes/release-notes-%npm_package_version%.md > release/release-notes-%npm_package_version%.html"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.2",
    "@angular/cdk": "^10.0.1",
    "@angular/common": "~10.0.2",
    "@angular/compiler": "~10.0.2",
    "@angular/core": "~10.0.2",
    "@angular/forms": "~10.0.2",
    "@angular/material": "^10.0.1",
    "@angular/platform-browser": "~10.0.2",
    "@angular/platform-browser-dynamic": "~10.0.2",
    "@angular/router": "~10.0.2",
    "@angular/service-worker": "^10.0.2",
    "karma-junit-reporter": "^2.0.1",
    "npm": "^6.14.5",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular/cli": "~10.0.1",
    "@angular/compiler-cli": "~10.0.2",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.12.48",
    "codelyzer": "^6.0.0-next.1",
    "compodoc": "0.0.41",
    "copyfiles": "^2.3.0",
    "cordova": "^9.0.0",
    "electron": "^9.1.0",
    "electron-builder": "^22.7.0",
    "electron-packager": "^15.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "markdown-to-html": "0.0.13",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  }
}
